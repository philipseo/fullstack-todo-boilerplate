{
  "name": "backend",
  "version": "0.0.1",
  "description": "backend",
  "license": "MIT",
  "keywords": [
    "Nodejs",
    "express",
    "graphql",
    "postgraphile",
    "typescript",
    "webpack",
    "eslint",
    "prettier",
    "husky"
  ],
  "main": "dist/index.js",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "scripts": {
    "serve": "webpack --config ./webpack.development.js & nodemon -e js,ts,graphql -x node dist/index.js",
    "serve:local": "yarn copy:local && yarn serve",
    "serve:development": "yarn copy:development && yarn serve",
    "serve:staging": "yarn copy:staging && yarn serve",
    "serve:production": "yarn copy:production && ./.env && yarn serve",
    "build": "webpack --config ./webpack.production.js",
    "build:local": "yarn copy:local && webpack --config ./webpack.production.js",
    "build:development": "yarn copy:development && webpack --config ./webpack.production.js",
    "build:staging": "yarn copy:staging && webpack --config ./webpack.production.js",
    "build:production": "yarn copy:production && webpack --config ./webpack.production.js",
    "copy:local": "cp ./.env.local ./.env",
    "copy:development": "cp ./.env.development ./.env",
    "copy:staging": "cp ./.env.staging ./.env",
    "copy:production": "cp ./.env.production ./.env",
    "clean": "rimraf ./dist",
    "lint": "eslint --ext .js,.ts",
    "lint:fix": "yarn lint -- --fix",
    "prettier": "prettier --ignore-path .prettierignore --write \"**/*.+(js|json|ts)\"",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "check-types": "tsc --project tsconfig.json --pretty --noEmit"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "connect-pg-simple": "^6.2.1",
    "connect-redis": "^5.1.0",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "graphql": "^15.4.0",
    "graphql-depth-limit": "^1.1.0",
    "graphql-shield": "^7.5.0",
    "graphql-voyager": "^1.0.0-rc.31",
    "helmet": "^4.4.1",
    "morgan": "^1.10.0",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "pg": "^8.5.1",
    "postgraphile": "^4.11.0",
    "redis": "^3.0.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@types/bcrypt": "^3.0.0",
    "@types/body-parser": "^1.19.0",
    "@types/compression": "^1.7.0",
    "@types/connect-pg-simple": "^4.2.2",
    "@types/connect-redis": "^0.0.16",
    "@types/cors": "^2.8.10",
    "@types/csurf": "^1.11.0",
    "@types/express": "^4.17.9",
    "@types/express-session": "^1.17.3",
    "@types/graphql": "^14.5.0",
    "@types/graphql-depth-limit": "^1.1.2",
    "@types/helmet": "^4.0.0",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.20",
    "@types/passport": "^1.0.6",
    "@types/passport-local": "^1.0.33",
    "@types/pg": "^7.14.9",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.17.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^4.3.7",
    "lint-staged": "^10.5.3",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-loader": "^8.0.14",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "webpack": "^5.14.0",
    "webpack-cli": "^4.3.1",
    "webpack-merge": "^5.7.3",
    "webpack-node-externals": "^2.5.2"
  }
}
