{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "backend",
  "license": "MIT",
  "keywords": [
    "react",
    "emotion",
    "graphql",
    "apollo-client",
    "typescript",
    "material-ui",
    "webpack",
    "eslint",
    "prettier",
    "husky"
  ],
  "scripts": {
    "serve": "webpack serve --config ./webpack.development.js",
    "serve:local": "yarn copy:local && yarn serve",
    "serve:development": "yarn copy:development && yarn serve",
    "serve:staging": "yarn copy:staging && yarn serve",
    "serve:production": "yarn copy:production && yarn serve",
    "build": "webpack --config ./webpack.production.js",
    "build:local": "yarn copy:local && yarn build",
    "build:development": "yarn copy:development && yarn build",
    "build:staging": "yarn copy:staging && yarn build",
    "build:production": "yarn copy:production && yarn build",
    "copy:local": "cp ./.env.local ./.env",
    "copy:development": "cp ./.env.development ./.env",
    "copy:staging": "cp ./.env.staging ./.env",
    "copy:production": "cp ./.env.production ./.env",
    "storybook": "start-storybook -p 6006",
    "clean": "rimraf ./dist",
    "lint": "eslint --ext .js,.ts",
    "lint:fix": "yarn lint -- --fix",
    "prettier": "prettier --ignore-path .prettierignore --write \"**/*.+(js|json|ts|tsx)\"",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "check-types": "tsc --project tsconfig.json --pretty --noEmit",
    "build-storybook": "build-storybook"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@apollo/client": "^3.3.11",
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.1.5",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@storybook/cli": "^6.1.20",
    "graphql": "^15.5.0",
    "notistack": "^1.0.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/preset-env": "^7.13.9",
    "@babel/preset-react": "^7.12.13",
    "@storybook/addon-a11y": "^6.1.20",
    "@storybook/addon-actions": "^6.1.20",
    "@storybook/addon-docs": "^6.1.20",
    "@storybook/addon-essentials": "^6.1.20",
    "@storybook/addon-knobs": "^6.1.20",
    "@storybook/addon-links": "^6.1.20",
    "@storybook/addon-storyshots": "^6.1.20",
    "@storybook/react": "^6.1.20",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/react-router-dom": "^5.1.7",
    "@types/webpack-env": "^1.16.0",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "autoprefixer": "^10.2.4",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^7.0.0",
    "css-loader": "^5.1.1",
    "dotenv-webpack": "^6.0.2",
    "eslint": "^7.21.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-webpack-plugin": "^2.5.2",
    "fork-ts-checker-webpack-plugin": "^6.1.0",
    "html-loader": "^2.1.1",
    "html-webpack-plugin": "^5.2.0",
    "husky": "^5.1.2",
    "lint-staged": "^10.5.4",
    "mini-css-extract-plugin": "^1.3.9",
    "prettier": "^2.2.1",
    "react-docgen-typescript": "^1.21.0",
    "storybook-addon-apollo-client": "^4.0.6",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.17",
    "typescript": "4.1.2",
    "webpack": "^5.24.2",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  }
}
